using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.IO;

namespace cw3
{

    class Program
    {
        public struct Samochod
        {
            public string cena;
            public string rocznik;
            public string przebieg;
            public string marka;
        }
        static void Wczytaj(StreamReader plik, ref Samochod parametr)
        {
                string input_line = plik.ReadLine();
                parametr.cena = input_line;
                input_line = plik.ReadLine();
                parametr.rocznik = input_line;
                input_line = plik.ReadLine();
                parametr.przebieg = input_line;
                input_line = plik.ReadLine();
                parametr.marka = input_line;
        }
        static void Wypisz(Samochod[] parametr2, int liczba_aut)
        {
            for (int a = 0; a < liczba_aut; a++)
            {
                Console.WriteLine(parametr2[a].cena);
                Console.WriteLine(parametr2[a].rocznik);
                Console.WriteLine(parametr2[a].przebieg);
                Console.WriteLine(parametr2[a].marka);
            }
        }
        static void Main(string[] args)
        {
            StreamReader samochody_txt = new StreamReader(@"D:/csharp/cw3/samochody.txt");
            string n_aut = samochody_txt.ReadLine();
            string[] length_n_aut = n_aut.Split(' ');
            int n = Convert.ToInt32(length_n_aut[0]);
            Samochod[] samochody = new Samochod[n];
            for (int j = 0; j < n; j++)
            {
                Wczytaj(samochody_txt, ref samochody[j]);
            }
            Wypisz(samochody, n);
            Console.ReadKey();
        }
    }
}
